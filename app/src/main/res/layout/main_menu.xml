<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainMenu"
    tools:showIn="@layout/activity_main_menu">

    <ListView
        android:id="@+id/lista_credenciales"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <Button
        android:id="@+id/nuevo_registro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/lista_credenciales"
        android:width="200dp"
        android:text="Nuevo Registro"
        android:textSize="13sp" />

    <Button
        android:id="@+id/importar_registro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lista_credenciales"
        android:layout_marginLeft="200dp"
        android:width="200dp"
        android:text="Importar Registro"
        android:textSize="13sp" />

</RelativeLayout>
